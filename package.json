{
  "name": "illumination",
  "version": "2.0.1",
  "description": "Interacting with Illumina ICAv2 API",
  "main": "dist/app.js",
  "scripts": {
    "dev": "nodemon -w src -w views -e ts,ejs --exec npm run dev:start",
    "dev:start": "npm-run-all build start2",
    "build-ts": "tsc",
    "build": "npm-run-all clean build-ts copy-assets",
    "clean": "rimraf dist/*",
    "copy-assets": "ts-node tools/copyStaticAssets.ts",
    "start": "npm run build && node .",
    "start2": "node .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/umccr/illumination.git"
  },
  "author": "https://github.com/pdiakumis",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/umccr/illumination/issues"
  },
  "homepage": "https://github.com/umccr/illumination#readme",
  "dependencies": {
    "axios": "^1.6.0",
    "chalk": "^4.1.1",
    "dayjs": "^1.10.6",
    "ejs": "^3.1.10",
    "express": "^4.19.2",
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^17.0.18",
    "@types/shelljs": "^0.8.11",
    "dotenv": "^16.0.0",
    "dotenv-expand": "^8.0.3",
    "express-list-endpoints": "^6.0.0",
    "fs-extra": "^10.0.0",
    "nodemon": "^3.1.4",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.5",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5"
  }
}
